@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/SuperAdmin/Views/Shared/SuperAdminMaster.cshtml.cshtml";
}

<!-- BEGIN CONTENT SECTION -->
	<div class="content-wrapper">
		
		<!-- First row of dashboard items -->
		<div class="row content-row">
		
			<!-- Pending Requests -->
			<div class="columns six">
				<h2>Pending Requests</h2>
				<div class="content-box">
					
					<table id="tableRequests">
						<tr>
							<td/>
						</tr>
					</table>
							
				</div>
			</div>
			
			<!-- Recent Activity -->
			<div class="columns six">
				<div class="select-bar">
					<select class="chzn-select width-150">
						<!-- This first <option> is needed here for the Chosen plugin: http://harvesthq.github.com/chosen/ -->
						<option></option>
						<option selected="">My Activity</option>
						<option>Client Activity</option>
					</select>
				</div>
				<h2>Recent Activity</h2>
				<div class="content-box">
					
					<table id="tableActivity">
						<tr>
							<td/>
						</tr>
					</table>
					
				</div>
			</div>
			
		</div>
		
		<!-- Second row of dashboard items -->
		<div class="row content-row">
		
			<!-- Latest Client Changes -->
			<div class="columns twelve">
				<h2>Latest Client Changes</h2>
				<div class="content-box">
					<table class="column-table">
						<tr>
							<td class="box-link"><a href="#" data-toggle="modal" data-target="#modalApproved"><h3>Recently<br>Approved</h3><strong>12</strong></a></td>
							<td class="box-link"><a href="#" data-toggle="modal" data-target="#modalPending"><h3>Pending<br>Approval</h3><strong>12</strong></a></td>
							<td class="box-link status1"><a href="#" data-toggle="modal" data-target="#modalMissing"><h3>Missing<br>Documents</h3><strong>12</strong></a></td>
							<td class="box-link status1"><a href="#" data-toggle="modal" data-target="#modalDenied"><h3>Approval<br>Denied</h3><strong>12</strong></a></td>
							<td class="box-link"><a href="#" data-toggle="modal" data-target="#modalNew"><h3>New<br>Account</h3><strong>12</strong></a></td>
							<td class="box-link"><a href="#" data-toggle="modal" data-target="#modalActive"><h3>Active<br>Account</h3><strong class="green">12</strong></a></td>
							<td class="box-link"><a href="#" data-toggle="modal" data-target="#modalInactive"><h3>Inactive<br>Account</h3><strong class="yellow">12</strong></a></td>
							<td class="box-link"><a href="#" data-toggle="modal" data-target="#modalDormant"><h3>Dormant<br>Account</h3><strong class="red">12</strong></a></td>
						</tr>
					</table>
				</div>
			</div>
			
		</div>
		
		<!-- Third row of dashboard items -->
		<div class="row content-row">
		
			<!-- Client Transactions -->
			<div class="columns eight">
				<div class="select-bar">
					<select id="drpTransacTypeSelector" class="chzn-select width-150">
						<!-- This first <option> is needed here for the Chosen plugin: http://harvesthq.github.com/chosen/ -->
						<option></option>
						<option value="deposit" selected="">Deposits</option>
						<option value="transfer">Internal Transfers</option>
						<option value="withdrawal">Withdrawals</option>
						<option value="conversion">Conversions</option>
					</select>
				</div>
				<h2>Client Transactions</h2>
				<div class="content-box">
					
					<!-- Deposits Table -->
					<div id="deposit_table" class="toggle-hide show">					
						<table id="tblDeposits">
							<tr>
								<td/>
							</tr>
						</table>
					</div>
					
					<!-- Transfers Table -->
					<div id="transfer_table" class="toggle-hide">
						<table id="tblTransfers">
							<tr>
								<td/>
							</tr>
						</table>
					</div>
					
					<!-- Withdrawels Table -->
					<div id="withdrawal_table" class="toggle-hide">	
						<table id="tblWithdrawals">
							<tr>
								<td/>
							</tr>
						</table>
					</div>
					
					<!-- Conversions Table -->
					<div id="conversion_table" class="toggle-hide">	
						<table id="tblConversions">
							<tr>
								<td/>
							</tr>
						</table>
					</div>
					
				</div>
			</div>
			
			<!-- Volume Overview -->
			<div class="columns four volume-overview">
				<div class="select-bar">
					<select class="chzn-select width-150">
						<!-- This first <option> is needed here for the Chosen plugin: http://harvesthq.github.com/chosen/ -->
						<option></option>
						<option selected="">Currencies</option>
						<option>CFD's</option>
					</select>
				</div>
				<h2>Volume Overview</h2>
				<div class="content-box">
					
					<div class="mtd-volume"><h3 class="h3 inline">Month to Date:</h3> 200,000,000</div>
					<div id="chartVolumeOverview" style="height: 207px;"></div>
					
				</div>
			</div>
			
		</div>
		
	</div>
	<!-- END CONTENT SECTION -->

<!-- BEGIN MODAL CONTENT SECTION -->
<div id="modalApproved" class="modal hide fade xlarge">
	
	<!-- Modal Header -->
	<div class="modal-header">
		<button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
		<h2 class="inline-block h2">Recently Approved Clients</h2>
